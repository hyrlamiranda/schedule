#matrizes( variaveis compostas homogeneas multidimensionais)
#mais dimensoes

var
  m : vetor[1..3, 1..2] de inteiro

inicio
    m[1,2] <- 4
    m[2,2] <- 5
    m[3,1] <- 8

#outro exemplo

var
  m: vetor[1..3, 1..2] de inteiro
  l,c: inteiro
inicio
    para l <- 1 ate 3 faca
      para c<- 1 ate 2 faca
          leia(m[l,c])
      fimpara
    fimpara
fimalgortimo
