#Adicional: Exibir a tabuada de um numero qualquer

algoritmo "Contarate10"
var
    Cont, N : Inteiro

inicio
      Cont <- 1
      #N <- 7
      Escreva("Quer ver a tabuada de qual numero?")
      Leia(N)
      Repita
      R <- N * Cont
          Escreval(N, "x", Cont)
          Cont <- Cont + 1
      Ate (Cont > 10)
fimalgoritmo
