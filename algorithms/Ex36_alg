algoritmo "sequenciaDeFibonacci"
var
    S1 , S2, S3, Seq: Inteiro
#fn = fn - 1 + fn - 2
inicio
      S1 <- 0
      Escreva(S1)
      S2 <- 1
      Escreval(S2)
      Para Seq := 1 ate 13 faca
          S3 := S1 + S2
          Escreva(S3)
            S1 := S2
            S2 := S3
      FimPara
fimalgoritmo
