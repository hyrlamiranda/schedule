#Repeticao
//exemplo

#Para c <- 1 Ate 10 passo 1 faca
    Escreval(c)
FimPara


algoritmo "contador1a10"
var
   C: Inteiro
inicio
    Para C <- 1 ate 10 faca
        Escreval(C)
    FimPara
fimalgoritmo

#Saltando de 2 em 2

#algoritmo "contador1a10"
var
   C: Inteiro
inicio
    Para C <- 1 ate 10 passo 2 faca
        Escreval(C)
    FimPara
fimalgoritmo
